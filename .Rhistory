# one time playing
# Plot money vs times of playing
bett1 = function(amount=1000, times=50, bet=10, pays=36, odd=37, m =2)
{
amount_series = c()
amount_series[1] = amount
new_bet = bet
for (i in 1:times)
{
win = rbernoulli(1, 1/odd)
if (win){
amount_series[i+1] = amount_series[i] + (pays-1)*new_bet
new_bet = bet
}
else {
amount_series[i+1] = amount_series[i] - new_bet
new_bet = m*new_bet
}
}
return(amount_series)
}
# When running out of money?
bett2 = function(amount=1000, times=50, bet=10, pays=36, odd=37, m =2)
{
amount_series = c()
amount_series[1] = amount
new_bet = bet
for (i in 1:times)
{
win = rbernoulli(1, 1/odd)
if (win){
amount_series[i+1] = amount_series[i] + (pays-1)*new_bet
new_bet = bet
}
else {
amount_series[i+1] = amount_series[i] - new_bet
new_bet = m*new_bet
}
if (amount_series[i+1]<0) break
}
return(amount_series)
}
